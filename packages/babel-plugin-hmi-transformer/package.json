{
  "name": "babel-plugin-hmi-transformer",
  "version": "0.0.1",
  "description": "Generate code at build-time",
  "main": "dist/index.js",
  "engines": {
    "node": "> 4",
    "npm": "> 3"
  },
  "scripts": {
    "build": "babel src --out-dir dist --extensions '.js,.ts,.tsx' --ignore '**/__tests__/**' --source-maps",
    "lint": "eslint ./src"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "babel-plugin",
    "babel",
    "code generation"
  ],
  "authors": [
    "Mike Grabowski mike@callstack.com",
    "Drapich Piotr <piotr.drapich@callstack.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.5.5",
    "require-from-string": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-typescript": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@callstack/eslint-config": "^7.0.0",
    "ast-pretty-print": "^2.0.1",
    "babel-plugin-tester": "^5.0.0",
    "eslint": "^6.3.0",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "extends": "@callstack"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/callstack-internal/react-xml.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/babel-plugin-boilerplate/issues"
  },
  "homepage": "https://github.com/callstack-internal/react-xml#readme"
}
